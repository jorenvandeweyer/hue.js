    "test": "jest ./dist"
  },
  "author": "Joren Vandeweyer",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.1.0",
    "eslint-plugin-jest": "^25.2.2",
    "jest": "^27.3.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "axios": "^0.23.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jorenvandeweyer/hue.js"
