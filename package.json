  "devDependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.7",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.5.0",
    "eslint-plugin-jest": "^23.20.0",
    "jest": "^26.1.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "axios": "^0.19.2"
  },
  "repository": {
    "type" : "git",
    "url" : "https://github.com/jorenvandeweyer/hue.js"
