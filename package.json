{
  "name": "hue",
  "version": "0.2.3",
  "description": "An easy to use wrapper for the Hue API in NodeJS",
  "keywords": [
    "hue",
    "hue-api",
    "javascript",
    "nodejs",
    "wrapper",
    "lights",
    "smart-home",
    "hue-bridge"
  ],
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
